# --- Nushell Aliases

# -- Keep native macos open
alias nu-open = open
alias open = ^open

alias ll = ls -l

alias vd = vimdiff

# -- Git --
alias g = git
alias gst = git status
alias ga = git add
alias gaa = git add --all
alias gc = git commit --verbose
alias grs = git restore
alias gf = git fetch
alias gp = git push
alias gd = git diff
alias gco = git checkout
alias gsw = git switch
alias gwt = git worktree

